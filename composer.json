{
    "name": "matthiasnoback/doctrine-orm-test-service-provider",
    "type": "library",
    "description": "Service provider for tests in need of a Doctrine entity manager",
    "keywords": ["PHPUnit", "Doctrine ORM", "service provider"],
    "homepage": "http://github.com/matthiasnoback/doctrine-orm-test-service-provider",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthias Noback",
            "email": "matthiasnoback@gmail.com",
            "homepage": "http://php-and-symfony.matthiasnoback.nl"
        }
    ],
    "require": {
        "matthiasnoback/phpunit-test-service-container": "~2.0",
        "matthiasnoback/doctrine-dbal-test-service-provider": "~2.0",
        "doctrine/orm": "~2.5@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "3.*"
    },
    "target-dir": "Noback/PHPUnitTestServiceContainer",
    "autoload": {
        "psr-0" : { "Noback\\PHPUnitTestServiceContainer" : "" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
