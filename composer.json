    }
  ],
  "require": {
    "php": "^7.0",
    "matthiasnoback/phpunit-test-service-container": "^3.0",
    "matthiasnoback/doctrine-dbal-test-service-provider": "^3.0",
    "doctrine/orm": "^2.5",
    "phpunit/phpunit": "^6.0"
  },
  "autoload": {
    "psr-4": {
